# APP Port ( Hypercorn FastAPI )
app_port=4433

# MongoDB Parameters
# Version is useful for old machines without AVX support
# 4.4 = CPU without AVX Support
# 5.0 = CPU with AVX Support
mongo_version=5.0
mongo_srv=mongo
mongo_db=/openquiz
mongo_port=27017
mongo_pass=
mongo_user=
mongo_health_interval=3s
mongo_health_timeout=3s
mongo_health_retries=3

# Mongo-express Parameters    
me_port=8081